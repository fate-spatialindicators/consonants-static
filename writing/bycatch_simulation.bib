  month = feb,
  year = {2009},
  keywords = {Bayesian,computational limitation,invasive species,multivariate spatial models,point-referenced,spatial modelling,spatial predictive process,species distributions},
  pages = {144--154},
  file = {Full Text PDF:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/FA884G28/Latimer et al. - 2009 - Hierarchical models facilitate spatial analysis of.pdf:application/pdf;Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/UKU2BGHN/abstract.html:text/html},
