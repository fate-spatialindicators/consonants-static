  keywords = {Ardea cinerea,Birds,Census data,Herons,Kalman filter,Lapwings,Leslie matrix models,Maximum likelihood,Prediction,Productivity,Ring–recovery data,State–space,Vanellus vanellus,Winter weather},
  pages = {540--547},
