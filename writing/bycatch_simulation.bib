  number = {9},
  urldate = {2016-10-26},
  journal = {ICES Journal of Marine Science: Journal du Conseil},
  author = {Schweigert, Jacob F. and Boldt, Jennifer L. and Flostrand, Linnea and Cleary, Jaclyn S.},
  month = jan,
  year = {2010},
  keywords = {bottom-up,collapse,competition,Pacific herring,recovery,top-down},
  pages = {1903--1913},
  file = {Full Text PDF:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/4FIGRMJ7/Schweigert et al. - 2010 - A review of factors limiting recovery of Pacific h.pdf:application/pdf;Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/UZXXNK9A/1903.html:text/html},
