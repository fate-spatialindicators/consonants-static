  month = jun,
  year = {2012},
  pages = {562--574},
  file = {Full Text PDF:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/FUVPGBV5/Mendelssohn et al. - 2012 - Oil Impacts on Coastal Wetlands Implications for .pdf:application/pdf;Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/WJNZB4I2/Oil-Impacts-on-Coastal-Wetlands-Implications-for.html:text/html},
