  month = jan,
  year = {2008},
  keywords = {acoustic survey,age-structured assessment,environmental impact assessment,“Exxon Valdez” oil spill,Pacific herring,Prince William Sound,stock assessment},
  pages = {44--50},
  file = {Full Text PDF:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/M9TGIPM8/Thorne and Thomas - 2008 - Herring and the “Exxon Valdez” oil spill an inves.pdf:application/pdf;Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/BJ7GZBS7/44.html:text/html},
